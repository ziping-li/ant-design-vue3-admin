{
  "name": "ant-design-vue3-admin",
  "version": "1.0.0",
  "description": "一个简单易配置的后台管理系统模板，基于 Vite2 + Vue3 + Typescript + Tsx，支持响应式布局，在 PC、平板和手机上均易使用",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.tsx src/",
    "lint:fix": "yarn run lint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/js-cookie": "^2.2.6",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vitejs/plugin-vue-jsx": "^1.0.2",
    "@vue/babel-plugin-jsx": "^1.0.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vuedx/typecheck": "^0.6.0",
    "@vuedx/typescript-plugin-vue": "^0.6.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "convue": "1.2.2",
    "css-loader": "^4.2.1",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "file-loader": "^6.0.0",
    "husky": "^4.3.8",
    "less": "^4.1.0",
    "less-loader": "^7.3.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^0.11.2",
    "prettier": "^2.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "ts-loader": "^8.0.4",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.0",
    "vite": "^2.0.0-beta.50",
    "vite-plugin-mock": "^2.2.0",
    "vue-loader": "^16.0.0-beta.7"
  },
  "dependencies": {
    "@convue-lib/scroll": "0.1.13",
    "@convue-lib/styles": "^0.1.12",
    "@convue-lib/utils": "^0.1.12",
    "ant-design-vue": "^2.0.1",
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0-rc.6",
    "vue-router": "4",
    "vuex": "^4.0.0"
  }
}
